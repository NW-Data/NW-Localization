	<string key="Torment_Cristina_Spawn" speaker="Cristina Krempel" comment="Enc_Quest_99A_MSQ_09_Torment.slice " rel_version="Quest_MSQ2_Revamp">¡Alto ahí!</string>
	<string key="Torment_Cristina_Defeat" speaker="Cristina Krempel" comment="Enc_Quest_99A_MSQ_09_Torment.slice " rel_version="Quest_MSQ2_Revamp">Mi mente... es mi...</string>
	<string key="Torment_Romanos_Spawn" speaker="Romanos Agallon" comment="Enc_Quest_99A_MSQ_09_Torment.slice " rel_version="Quest_MSQ2_Revamp">¡Jamás os rindáis!</string>
	<string key="Torment_Romanos_Defeat" speaker="Romanos Agallon" comment="Enc_Quest_99A_MSQ_09_Torment.slice " rel_version="Quest_MSQ2_Revamp">Detrás de mí...</string>
	<string key="Torment_Soulwardens_Spawn" speaker="Torment of the Soulwardens" comment="Enc_Quest_99A_MSQ_09_Torment.slice " rel_version="Quest_MSQ2_Revamp">¡Guardianes del alma! ¡A las armas!</string>
	<string key="Torment_Soulwardens_Defeat" speaker="Torment of the Soulwardens" comment="Enc_Quest_99A_MSQ_09_Torment.slice " rel_version="Quest_MSQ2_Revamp">Yo... no me rindo... jamás...</string>
	<string key="Heartforge_Guardian_CombatBark_01" speaker="@AncientGuardian_Mage_MSQ_Named_02_Encounter_Boss_VitalsName" comment="AI_Heartforge_Guardian.slice" rel_version="Quest_MSQ2_Revamp">Has entrado en terreno sagrado. Largo.</string>
	<string key="Heartforge_Guardian_CombatBark_02" speaker="@AncientGuardian_Mage_MSQ_Named_02_Encounter_Boss_VitalsName" comment="AI_Heartforge_Guardian.slice" rel_version="Quest_MSQ2_Revamp">Fuera. Aquí solo te espera la muerte.</string>
	<string key="Heartforge_Guardian_CombatBark_03" speaker="@AncientGuardian_Mage_MSQ_Named_02_Encounter_Boss_VitalsName" comment="AI_Heartforge_Guardian.slice" rel_version="Quest_MSQ2_Revamp">Vemos tu fuerza. Acércate a la forja y acepta tu destino.</string>
	<string key="Overseer_Zane_CombatBark_01" speaker="@Damned_1H_Swordsman_M_Named_07_VitalsName" comment="AI_Overseer_Zane.slice" rel_version="Quest_MSQ2_Revamp">TEXTO PH: ¡Tu poder será mío!</string>
	<string key="Overseer_Zane_CombatBark_02" speaker="@Damned_1H_Swordsman_M_Named_07_VitalsName" comment="AI_Overseer_Zane.slice" rel_version="Quest_MSQ2_Revamp">TEXTO PH: Qué poder... tan abrumador...</string>
	<string key="99A_MSQ_07_Enc_Bark_OverSeerZan_Spawn" speaker="@Damned_1H_Swordsman_M_Named_07_VitalsName" comment="AI_Overseer_Zane.slice" rel_version="Quest_MSQ2_Revamp">Tu arrogancia será tu final.</string>
	<string key="99A_MSQ_07_Enc_Bark_OverSeerZan_EngageTendrils" speaker="@Damned_1H_Swordsman_M_Named_07_VitalsName" comment="AI_Overseer_Zane.slice" rel_version="Quest_MSQ2_Revamp">¡Te unirás a nosotros o te absorberemos la vida!</string>
	<string key="99A_MSQ_07_Enc_Bark_OverSeerZan_EngageZane" speaker="@Damned_1H_Swordsman_M_Named_07_VitalsName" comment="AI_Overseer_Zane.slice" rel_version="Quest_MSQ2_Revamp">Tu resistencia... me divierte.</string>
	<string key="99A_MSQ_07_Enc_Bark_OverSeerZan_Defeat" speaker="@Damned_1H_Swordsman_M_Named_07_VitalsName" comment="AI_Overseer_Zane.slice" rel_version="Quest_MSQ2_Revamp">No puedes derrotarnos a todos...</string>
	<string key="Helvan_CombatBark_01" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">TEXTO PH: ¡Saludos!</string>
	<string key="Helvan_CombatBark_02" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">TEXTO PH: ¡Disparadles! ¡A estribor!</string>
	<string key="Helvan_CombatBark_03" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">TEXTO PH: ¡Disparadles! ¡A babor!</string>
	<string key="Helvan_CombatBark_04" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">TEXTO PH: ¡Rufianes! Me encargaré personalmente de vosotros.</string>
	<string key="Helvan_CombatBark_05" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">TEXTO PH: Me voy a pique...</string>
	<string key="Helvan_GreekFire_line1" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">¡Arde, sabandija!</string>
	<string key="Helvan_CannonFire_line1" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">¡A estribor, alimaña!</string>
	<string key="Helvan_CannonFire_line2" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">¡A babor, sucia rata!</string>
	<string key="Helvan_Death_line1" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">Una victoria efímera. ¡Pronto serás como yo!</string>
	<string key="Helvan_JumpsDown_line1" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">¡Arrrr, grumete!</string>
	<string key="Imhotep_CombatBark_01" speaker="@Imhotep_VitalsName" comment="AI_Imhotep.slice" rel_version="Territory_Brimstone_Sands">¡Observa el poder de los antiguos! ¡Siente su ira!</string>
	<string key="Imhotep_CombatBark_02" speaker="@Imhotep_VitalsName" comment="AI_Imhotep.slice" rel_version="Territory_Brimstone_Sands">¡Soy el cancerbero de los misterios! ¡El guardián de los secretos!</string>
	<string key="Imhotep_CombatBark_03" speaker="@Imhotep_VitalsName" comment="AI_Imhotep.slice" rel_version="Territory_Brimstone_Sands">¡Ni siquiera tú eres rival para los antiguos! ¡Deja ya de luchar!</string>
	<string key="Imhotep_CombatBark_04" speaker="@Imhotep_VitalsName" comment="AI_Imhotep.slice" rel_version="Territory_Brimstone_Sands">¡Pon fin a este sinsentido! ¡Únete a mí! ¡Puedes ayudarme a apaciguar a los dioses!</string>
	<string key="Imhotep_CombatBark_05" speaker="@Imhotep_VitalsName" comment="AI_Imhotep.slice" rel_version="Territory_Brimstone_Sands">¡Aetérnum pertenece a los antiguos! ¡Debemos dejar que la recuperen! ¡No ha sido nada más que una maldición para nuestra gente!</string>
	<string key="Heru_Intro_A" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Alto, transeúnte! ¡Nadie pasará por la Puerta de la Noche!</string>
	<string key="Heru_Intro_B" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Abandona ahora o enfréntate a nuestra ira!</string>
	<string key="Heru_CombatTauntA" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡No se te da la bienvenida aquí!</string>
	<string key="Heru_CombatTauntB" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Siente la luz!</string>
	<string key="Heru_CombatTauntC" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Pagarás por tu irreverencia!</string>
	<string key="Heru_CombatTauntD" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Arde, cobarde!</string>
	<string key="Heru_Activate" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Me alzo de nuevo, maestros!</string>
	<string key="Heru_Deactivate" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Mi voluntad se quiebra! No merezco la luz de los maestros...</string>
	<string key="Heru_Doom_Intro" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">No puedes huir...</string>
	<string key="Heru_Doom_Explo" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡de la ira de los maestros!</string>
	<string key="Heru_Judgement" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡La palabra de los maestros es la ley!</string>
	<string key="Anpu_Intro" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Detente, ruin! ¡No entrarás aquí! ¡Tus lamentos resonarán como el viento!</string>
	<string key="Anpu_CombatTauntA" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Márchate, ruin!</string>
	<string key="Anpu_CombatTauntC" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">Oigo la llamada de los maestros.</string>
	<string key="Anpu_CombatTauntD" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">Tu arrogancia... ¡será castigada!</string>
	<string key="Anpu_CombatTauntE" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">¡La oscuridad me reclama! He fallado mi tarea sagrada...</string>
	<string key="Anpu_CombatTauntF" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Arde, cobarde!</string>
	<string key="Anpu_Activate" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">¡La luz sagrada me bendice!</string>
	<string key="Anpu_BurningCharge" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">¡La puerta será purificada!</string>
	<string key="Crassus_Intro" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡No puedes oponerte al poder de Roma!</string>
	<string key="Crassus_ChaosLance" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Muere, chucho!</string>
	<string key="Crassus_ChaosSpin" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Basta, basura!</string>
	<string key="Crassus_Corrupted_Blob" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Haré arrodillarse a esta tierra!</string>
	<string key="Crassus_CorruptedPhalanx" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Somos una marea imparable!</string>
	<string key="Crassus_FireRain" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡La gloria de Roma incendiará Aetérnum!</string>
	<string key="Crassus_Enrage" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Enviad a esas bestias al inframundo!</string>
	<string key="Crassus_PhaseTransition2" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Siente la rabia que arde dentro!</string>
	<string key="Crassus_PhaseTransition3" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Contempla el poder que me otorgaron!</string>
	<string key="Crassus_ChaosVolley" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Este poder es nuestro, ¡no de los dioses!</string>
	<string key="Crassus_ShieldRush" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Largo, can!</string>
	<string key="Crassus_SplitAttack" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Hemos pagado este poder con nuestra alma!</string>
	<string key="Crassus_Victory" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">¡Alabado sea el poder de Roma!</string>
	<string key="Crassus_Death" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Era todo mentira... Por favor, perdóname, Evandrus...</string>
	<string key="Baalphazu_Intro_Bark1" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Insensatos! ¡Os ahogaré bajo las sombras y el miedo! ¡El Señor Tenebroso tendrá su festín macabro!</string>
	<string key="Baalphazu_ShortBark_01" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡El miedo os consume!</string>
	<string key="Baalphazu_ShortBark_02" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Ansío más!</string>
	<string key="Baalphazu_ShortBark_03" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Que empiece el banquete!</string>
	<string key="Baalphazu_MedBark_01" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Vuestro terror puro nos sostiene!</string>
	<string key="Baalphazu_MedBark_02" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Las sombras reclamarán vuestra alma!</string>
	<string key="Baalphazu_MedBark_03" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡No hay escapatoria al miedo!</string>
	<string key="Baalphazu_LongBark_01" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Alzaos, sombras de la desesperación! ¡Propagad el terror!</string>
	<string key="Baalphazu_LongBark_02" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Recogeremos el miedo que mi terrible maestro siembra!</string>
	<string key="Baalphazu_LongBark_03" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡El horror llena la tierra! ¡Vuestro miedo gélido es delicioso!</string>
	<string key="Baalphazu_EnterWeak_01" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Maldición!</string>
	<string key="Baalphazu_EnterWeak_02" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Necio insolente!</string>
	<string key="Baalphazu_EnterWeak_03" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Carne pútrida!</string>
	<string key="Baalphazu_ExitWeak_01" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Púdrete en la oscuridad, lombriz!</string>
	<string key="Baalphazu_ExitWeak_02" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Sufrirás, gusano!</string>
	<string key="Baalphazu_ExitWeak_03" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Maldita insolencia!</string>
	<string key="Baalphazu_Victory_Bark1" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Horrores infinitos reclamarán la tierra! ¡Un ávido festín para mi maestro!</string>
	<string key="Baalphazu_Death_Bark1" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">¡Mi maestro será saciado! ¡No hay escapatoria al terror! Ninguna luz desterrará la oscuridad eterna...</string>
	<string key="Apophis_CombatTaunt_A" speaker="Apophis" comment="Sand_Elemental_QuestBoss.slice" rel_version="Territory_Brimstone_Sands">¡Prepárate para ser juzgado!</string>
	<string key="Apophis_CombatTaunt_B" speaker="Apophis" comment="Sand_Elemental_QuestBoss.slice" rel_version="Territory_Brimstone_Sands">¡Largo hemos sufrido vuestra presencia!</string>
	<string key="Apophis_CombatTaunt_C" speaker="Apophis" comment="Sand_Elemental_QuestBoss.slice" rel_version="Territory_Brimstone_Sands">¡Ansío la destrucción!</string>
	<string key="Apophis_CombatTaunt_D" speaker="Apophis" comment="Sand_Elemental_QuestBoss.slice" rel_version="Territory_Brimstone_Sands">¡Sacia tu ansia, Gran Devorador!</string>
	<string key="Apophis_Intro" speaker="Apophis" comment="Sand_Elemental_QuestBoss.slice" rel_version="Territory_Brimstone_Sands">¡Gusanos insignificantes!</string>
	<string key="Apophis_Death" speaker="Apophis" comment="Sand_Elemental_QuestBoss.slice" rel_version="Territory_Brimstone_Sands">¡No eres nada!</string>
	<string key="Flamekeeper_CombatBark_01" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">El fuego purificador vaticinó este momento.</string>
	<string key="Flamekeeper_CombatBark_02" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">¡A las armas, guerreros del fuego!</string>
	<string key="Flamekeeper_CombatBark_03" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">¡Destruid a este profano!</string>
	<string key="Flamekeeper_CombatBark_04" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">¡Guerrero infernal! ¡Ataca!</string>
	<string key="Flamekeeper_CombatBark_05" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">El fuego me ilumina el camino.</string>
	<string key="Flamekeeper_CombatBark_06" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">¡Llama del paraíso!</string>
	<string key="Flamekeeper_CombatBark_07" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">¡Ven, Ahriman! ¡Consúmelo todo!</string>
	<string key="Flamekeeper_CombatBark_08" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">¡Conflagración hirviente!</string>
	<string key="Flamekeeper_CombatBark_09" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">¡Que ardan! ¡Que ardan! ¡Ja, ja!</string>
	<string key="Flamekeeper_CombatBark_10" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">¡Invoco al vacío ardiente!</string>
	<string key="Flamekeeper_CombatBark_11" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">¡Alzaos, despiadadas garras de daeva!</string>
	<string key="Flamekeeper_CombatBark_12" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">¡Levanta barreras ardientes!</string>
	<string key="Flamekeeper_CombatBark_13" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">¡Enciende la mecha de mi aliento! ¡Prende la llama de mi alma!</string>
	<string key="Flamekeeper_CombatBark_14" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">La pureza del fuego… me acoge en su seno…</string>
	<string key="BrotherUmberto_Intro_Human" speaker="@UmbertoHuman_SubName" comment="brother_umberto_human.slice" rel_version="Quest_MSQ2_Brightwood">¡No me arrepentiré! Ni pereceré. ¡Es tu alma la que debe ser castigada!</string>
	<string key="BrotherUmberto_Death_Human" speaker="@UmbertoHuman_SubName" comment="brother_umberto_human.slice" rel_version="Quest_MSQ2_Brightwood">¡Los justos siempre regresan!</string>
	<string key="BrotherUmberto_Intro_Dark_Alt" speaker="@UmbertoSpirit_SubName" comment="brother_umberto.slice" rel_version="Quest_MSQ2_Brightwood">Mi alma no se extinguirá tan fácilmente…</string>
	<string key="BrotherUmberto_Death_Dark" speaker="@UmbertoSpirit_SubName" comment="io_caerdun_99b_msq_10_finalseal.slice" rel_version="Quest_MSQ2_Brightwood">Estoy… liberado.</string>
	<string key="Season_02_Assassin_Ambush_1" speaker="Delirvesh the Sand Drinker" comment="Enc_Season_02_EggAmbush_02.slice" rel_version="Season_2_Perm">¡El huevo debe quedarse donde está! ¡De peores enemigos lo he protegido!</string>
	<string key="Season_02_Assassin_Ambush_2" speaker="Blood of the Sands Assassin" comment="Enc_Season_02_EggAmbush_02.slice" rel_version="Season_2_Perm">¡Es nuestra oportunidad para coger el huevo!</string>
	<string key="Season_02_Assassin_Ambush_2a" speaker="Blood of the Sands Assassin" comment="Enc_Season_02_EggAmbush_02.slice" rel_version="Season_2_Perm">¡Dame ese huevo!</string>
	<string key="Season_02_Assassin_Ambush_3" speaker="@Bog_Monster_Season_02_VitalsName" comment="Enc_Season_02_EggAmbush_03.slice" rel_version="Season_2_Perm">¡Nadie te quiere aquí! Lárgate ya.</string>
	<string key="Season_02_Assassin_Ambush_3a" speaker="@Bog_Monster_Season_02_VitalsName" comment="Enc_Season_02_EggAmbush_03.slice" rel_version="Season_2_Perm">Has llegado a las puertas… ¡Atacad!</string>
	<string key="Season_02_Assassin_Ambush_3b" speaker="@Bog_Monster_Season_02_VitalsName" comment="Enc_Season_02_EggAmbush_03.slice" rel_version="Season_2_Perm">¡Alzaos y protegedlo!</string>
	<string key="Herikai_CombatBark_Intro" speaker="Herikai" comment="season_02_herikai.slice" rel_version="Season_2_Temp">¡Malek dijo que vendrías a por el huevo! ¡Demasiado tarde, profiereblasfemias! Ahora nos pertenece.</string>
	<string key="VOX_BarkA" speaker="Yonas" comment="yonas_msq2_combat.slice" rel_version="Quest_MSQ2_Edengrove">No quiero hacer esto... ¡Detenme!</string>
	<string key="VOX_BarkB" speaker="Yonas" comment="yonas_msq2_combat.slice" rel_version="Quest_MSQ2_Edengrove">¡La gema del alma! ¡Destrúyela!</string>
	<string key="VOX_BarkB_Quick" speaker="Yonas" comment="yonas_msq2_combat.slice" rel_version="Quest_MSQ2_Edengrove">¡La gema del alma! ¡Destrúyela!</string>
	<string key="VOX_BarkDeath" speaker="Yonas" comment="yonas_msq2_combat.slice" rel_version="Quest_MSQ2_Edengrove">Hazlo, te lo ruego... Acaba con esto...</string>
	<string key="VOX_Saved" speaker="Yonas" comment="yonas_msq2_combat.slice" rel_version="Quest_MSQ2_Edengrove">Me has salvado. Gracias...</string>
	<string key="Adiana_Motherwell_Intro" speaker="Adiana" comment="enc_quest_motherwell.slice" rel_version="Quest_MSQ2_Edengrove">¿Quién osa entrar en el Pozo Madre? ¿Tú? ¿Cómo… te atreves?</string>
	<string key="Adiana_Motherwell_Phase1" speaker="Adiana" comment="enc_quest_motherwell.slice" rel_version="Quest_MSQ2_Edengrove">¡Detén este despropósito de inmediato!</string>
	<string key="Adiana_Motherwell_Phase2" speaker="Adiana" comment="enc_quest_motherwell.slice" rel_version="Quest_MSQ2_Edengrove">¡No sufriremos por tu culpa!</string>
	<string key="Adiana_Motherwell_Death" speaker="Adiana" comment="enc_quest_motherwell.slice" rel_version="Quest_MSQ2_Edengrove">Me has desafiado, criatura humana, ¡y no lo olvidaré!</string>
	<string key="Isabella_YonasTaunt_1" speaker="Isabella" comment="yonascave_99e_msq_isabella_noai.slice" rel_version="Quest_MSQ2_Edengrove">¡Anda, mira quién ha venido! Oh, pero te has perdido toda la diversión.</string>
	<string key="Isabella_YonasTaunt_2" speaker="Isabella" comment="yonascave_99e_msq_isabella_noai.slice" rel_version="Quest_MSQ2_Edengrove">¡Extraer una gema del alma es mucho más fácil de lo que esperaba!</string>
	<string key="Isabella_YonasTaunt_3" speaker="Isabella" comment="yonascave_99e_msq_isabella_noai.slice" rel_version="Quest_MSQ2_Edengrove">Creías que estabas empezando a tomar ventaja, pero nunca tuviste la más mínima oportunidad.</string>
	<string key="Isabella_YonasTaunt_4" speaker="Isabella" comment="yonascave_99e_msq_isabella_noai.slice" rel_version="Quest_MSQ2_Edengrove">Lo poco que has conseguido, me lo he quedado yo.</string>
	<string key="Isabella_YonasTaunt_5" speaker="Isabella" comment="yonascave_99e_msq_isabella_noai.slice" rel_version="Quest_MSQ2_Edengrove">Aquí tienes a tu mentor, a tu amigo… o lo que queda de él.</string>
	<string key="VOX_Combat_1" speaker="Medusa" comment="ai_fl_medusa.slice" rel_version="Territory_FirstLight">Tus defensas no te salvarán.</string>
	<string key="VOX_Combat_2" speaker="Medusa" comment="ai_fl_medusa.slice" rel_version="Territory_FirstLight">Inmortaliza su belleza.</string>
	<string key="VOX_Combat_3" speaker="Medusa" comment="ai_fl_medusa.slice" rel_version="Territory_FirstLight">Los dioses antiguos se alzarán de nuevo.</string>
