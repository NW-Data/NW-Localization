	<string key="Referee_InProgress_02" speaker="@ui_playertitle_pve_killuniquenamed_corrupt" comment="Dungeon_CutlassKeys00_Referee.slice" rel_version="Expedition_CutlassKeys_00">¿Crees que te has ganado el puesto?</string>
