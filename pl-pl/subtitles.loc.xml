	<string key="Flamekeeper_CombatBark_04" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">Piekielny wojowniku! Uderzaj!</string>
	<string key="Flamekeeper_CombatBark_05" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">Mój ogień oświetli drogę.</string>
	<string key="Flamekeeper_CombatBark_06" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">Niebiański płomień!</string>
	<string key="Flamekeeper_CombatBark_07" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">Naprzód, Arymanie! Pożryj wszystko!</string>
	<string key="Flamekeeper_CombatBark_08" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">Gore! Gore!</string>
	<string key="Flamekeeper_CombatBark_09" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">Płoń! Płoń! Ha, ha!</string>
	<string key="Flamekeeper_CombatBark_10" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">Przyzywam płonącą pustkę!</string>
	<string key="Flamekeeper_CombatBark_11" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">Oto ogniste szpony dewy!</string>
	<string key="Flamekeeper_CombatBark_12" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">Powstańcie, ogniste zapory!</string>
	<string key="Flamekeeper_CombatBark_13" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">Podpal me tchnienie! Rozpal mą duszę!</string>
	<string key="Flamekeeper_CombatBark_14" speaker="Hadish Azar" comment="Evil_Knight_Flamekeeper.slice" rel_version="Season_1_Perm">Oczyszczający ogień… pochłania mnie…</string>
	<string key="BrotherUmberto_Intro_Human" speaker="@UmbertoHuman_SubName" comment="brother_umberto_human.slice" rel_version="Quest_MSQ2_Brightwood">Niczego nie żałuję! I nie polegnę. To ty stracisz swą duszę!</string>
