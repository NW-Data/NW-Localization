<resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<string key="cutscene_line1">OJCIEC RUSSO: Opowiadane historie… nie wspominają o wszystkim.</string>
	<string key="cutscene_line2">Ta wyspa jest zaiste miejscem z bajki.</string>
	<string key="cutscene_line3">Można znaleźć na niej potęgę i ogromne bogactwa.</string>
	<string key="cutscene_line4">Ale wielu przed tobą zniknęło bez śladu, a słuch po nich zaginął.</string>
	<string key="cutscene_line5">Byłem świadkiem przerażenia… obłędu, który opanował załogę.</string>
	<string key="cutscene_line6">Takiego zła świat jeszcze nie widział.</string>
	<string key="cutscene_line7">Jak przeżyłem… nie wiem sam.</string>
	<string key="cutscene_line8">Zawartość tego pudełka nie pochodzi z tego świata.</string>
	<string key="cutscene_line9">Zaprowadzi cię do tamtego, gdziekolwiek i czymkolwiek jest…</string>
	<string key="cutscene_line10">W drogę, kapitanie.</string>
	<string key="cutscene_line11">Wyznacz swój kurs i los.</string>
	<string key="cutscene_line12">Pomodlę się za dusze twojej załogi.</string>
	<string key="simongrey_intro_line1" speaker="Simon Grey" comment="Undead_Brute_Boss.slice">Co się stało? WSPANIALE!</string>
	<string key="simongrey_intro_line2" speaker="Simon Grey" comment="Undead_Brute_Boss.slice">Kto… Intruzi!</string>
	<string key="simongrey_intro_line3" speaker="Simon Grey" comment="Undead_Brute_Boss.slice">Nie weźmiesz sobie moich tajemnic!</string>
	<string key="sirenqueen_combat_line1" speaker="@Undead_Siren_VitalsName" comment="Undead_LostSiren.slice">Wszystko ci odbiorę.</string>
	<string key="sirenqueen_combat_line2" speaker="@Undead_Siren_VitalsName" comment="Undead_LostSiren.slice">Niech cię echo pochłonie!</string>
	<string key="sirenqueen_combat_line3" speaker="@Undead_Siren_VitalsName" comment="Undead_LostSiren.slice">Nic… nie słyszę…</string>
	<string key="caretaker_intro_line1" speaker="@Bog_Monster_VitalsName" comment="BASE_Bog_Monster.slice">Moje roślinki!</string>
	<string key="caretaker_intro_line2" speaker="@Bog_Monster_VitalsName" comment="BASE_Bog_Monster.slice">Moje kwiatuszki!</string>
	<string key="caretaker_intro_line3" speaker="@Bog_Monster_VitalsName" comment="BASE_Bog_Monster.slice">MOJE DZIECIĄTKA!</string>
	<string key="EmpressZhou_intro_1" speaker="Empress Zhou" comment="Dynasty_Empress.slice">Jak śmiesz…</string>
	<string key="EmpressZhou_intro_2" speaker="Empress Zhou" comment="Dynasty_Empress.slice">próbować mi zaszkodzić.</string>
	<string key="EmpressZhou_intro_3" speaker="Empress Zhou" comment="Dynasty_Empress.slice">Cesarzowej świata…</string>
	<string key="EmpressZhou_intro_4" speaker="Empress Zhou" comment="Dynasty_Empress.slice">starego i nowego!</string>
	<string key="EmpressZhou_intro_5" speaker="Empress Zhou" comment="Dynasty_Empress.slice">To dla ciebie za wysokie progi.</string>
	<string key="EmpressZhou_CorruptionWave2" speaker="Empress Zhou" comment="Dynasty_Empress.slice">Nie zbliżaj się!</string>
	<string key="EmpressZhou_Phase2A" speaker="Empress Zhou" comment="Dynasty_Empress.slice">Nie zasługujesz na śmierć z moich rąk.</string>
	<string key="EmpressZhou_Phase2B" speaker="Empress Zhou" comment="Dynasty_Empress.slice">Zdechniesz, czując tylko żal.</string>
	<string key="EmpressZhou_Death1" speaker="Empress Zhou" comment="Dynasty_Empress.slice">Kategorycznie… odmawiam…</string>
	<string key="EmpressZhou_Death2" speaker="Empress Zhou" comment="Dynasty_Empress.slice">Moje… doskonałe… imperium…</string>
	<string key="EmpressZhou_Alert" speaker="Empress Zhou" comment="Dynasty_Empress.slice">Czy wszystko muszę robić sama?</string>
	<string key="greundgul_taunt_1" speaker="@AncientGuardian_Heavy_Boss_Named_DG_Everfall_00_VitalsName" comment="AncientGuardian_Heavy_Boss.slice">Złodzieje. Mnie nie okradniecie.</string>
	<string key="greundgul_taunt_2" speaker="@AncientGuardian_Heavy_Boss_Named_DG_Everfall_00_VitalsName" comment="AncientGuardian_Heavy_Boss.slice">Jesteście słabi. Poddajcie się.</string>
	<string key="greundgul_taunt_3" speaker="@AncientGuardian_Heavy_Boss_Named_DG_Everfall_00_VitalsName" comment="AncientGuardian_Heavy_Boss.slice">Ha, ha! Lepiej uważajcie.</string>
	<string key="greundgul_taunt_4" speaker="@AncientGuardian_Heavy_Boss_Named_DG_Everfall_00_VitalsName" comment="AncientGuardian_Heavy_Boss.slice">Strażnicy, zatrzymajcie ich.</string>
	<string key="greundgul_taunt_5" speaker="@AncientGuardian_Heavy_Boss_Named_DG_Everfall_00_VitalsName" comment="AncientGuardian_Heavy_Boss.slice">Ani kroku dalej.</string>
	<string key="greundgul_taunt_6" speaker="@AncientGuardian_Heavy_Boss_Named_DG_Everfall_00_VitalsName" comment="AncientGuardian_Heavy_Boss.slice">Zakaz wstępu.</string>
	<string key="greundgul_taunt_7" speaker="@AncientGuardian_Heavy_Boss_Named_DG_Everfall_00_VitalsName" comment="AncientGuardian_Heavy_Boss.slice">Pożałujecie przyjścia tu.</string>
	<string key="greundgul_intro_1" speaker="@AncientGuardian_Heavy_Boss_Named_DG_Everfall_00_VitalsName" comment="AncientGuardian_Heavy_Boss.slice">Złodzieje. Nie ukradniecie wieczności.</string>
	<string key="NagaAncient_Intro_1" speaker="Cilla" comment="AncientGuardian_Naga.slice">Wyczuwam twój podstęp…</string>
	<string key="NagaAncient_Intro_2" speaker="Cilla" comment="AncientGuardian_Naga.slice">Arogancja przyniesie ci zagładę!</string>
	<string key="TorsoBoss_Intro_1" speaker="Chardis" comment="Torso_Boss.slice">Nie odrodzisz się.</string>
	<string key="NagaAngryEarth_Intro_1" speaker="@Naga_AngryEarth_VitalsName" comment="Naga_Angry_Earth.slice">Ludzkie ścierwo!</string>
	<string key="NagaAngryEarth_Intro_2" speaker="@Naga_AngryEarth_VitalsName" comment="Naga_Angry_Earth.slice">Nie wiesz, co to wyższe dobro.</string>
	<string key="CaptainThorpe_CombatTaunt_1" speaker="@9911_MSQ_Thorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Teraz znam tajemnicę wyspy! Teraz znam prawdziwą potęgę!</string>
	<string key="CaptainThorpe_CombatTaunt_2" speaker="@9911_MSQ_Thorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Gdzie… moje… pudełko!</string>
	<string key="CaptainThorpe_CombatTaunt_3" speaker="@9911_MSQ_Thorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Czuję się już o wiele lepiej!</string>
	<string key="CaptainThorpe_CombatTaunt_4" speaker="@9911_MSQ_Thorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Zgniotę cię obcasem, robaku!</string>
	<string key="CaptainThorpe_CombatTaunt_5" speaker="@9911_MSQ_Thorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Śmierć na morzu byłaby dla ciebie lepszym losem!</string>
	<string key="CaptainThorpe_CombatTaunt_6" speaker="@9911_MSQ_Thorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Nie możesz równać się z mocą, którą dało mi skażenie.</string>
	<string key="CaptainThorpe_CombatTaunt_7" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Poddaj się skażeniu! Tylko dzięki temu zachowasz duszę!</string>
	<string key="CaptainThorpe_CombatTaunt_8" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Zaraz zmiażdżę twoją duszę!</string>
	<string key="CaptainThorpe_CombatTaunt_9" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Ha! Żałosne!</string>
	<string key="CaptainThorpe_CombatTaunt_10" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Ty jesteś przeszłością, a ja – przyszłością!</string>
	<string key="CaptainThorpe_CombatTaunt_11" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Posmakujesz skażonej stali!</string>
	<string key="CaptainThorpe_CombatTaunt_12" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Czeka cię unicestwienie!</string>
	<string key="CaptainThorpe_CombatTaunt_13" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">NIE! Znowu!</string>
	<string key="CaptainThorpe_CombatTaunt_14" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">To nasze ostatnie spotkanie.</string>
	<string key="CaptainThorpe_CombatTaunt_15" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Teraz służę Burzy… z własnej woli.</string>
	<string key="CaptainThorpe_CombatTaunt_16" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Nie stanowisz dla nas trudności.</string>
	<string key="CaptainThorpe_CombatTaunt_17" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Przysłała mnie… bym cię zniszczył!</string>
	<string key="CaptainThorpe_CombatTaunt_18" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Widzisz? To cały czas była jej dusza!</string>
	<string key="CaptainThorpe_Combat_beforesurge" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Tak, moja pani… Zwrócę to, co zostało odebrane…</string>
	<string key="CaptainThorpe_Combat_aftersurge" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Czuję, jak ogarnia mnie twe błogosławieństwo… Wieczna chwała będzie nasza…</string>
	<string key="CaptainThorpe_Combat_finale" speaker="@9929_MSQ_RecliningThorpe" comment="Corrupted_Captain_Thorpe_RestlessShoresBoss.slice">Zwycięstwo jest nasze, pani… ta dusza należy do ciebie…</string>
	<string key="Isabella_CombatTaunt_1" speaker="Isabella" comment="Isabella_Base.slice">Nie trzeba było tu przychodzić.</string>
	<string key="Isabella_CombatTaunt_2" speaker="Isabella" comment="Isabella_Base.slice">Na szczęście mam sługi od zajmowania się śmieciami, które wyrzuca morze.</string>
	<string key="Isabella_CombatTaunt_3" speaker="Isabella" comment="Isabella_Base.slice">Poddasz się skażeniu albo czeka cię zniszczenie.</string>
	<string key="Isabella_CombatTaunt_4" speaker="Isabella" comment="Isabella_Base.slice">Ostatnie ćwiczenia strzeleckie, nim moje okręty wyruszą skazić świat!</string>
	<string key="Isabella_CombatTaunt_5" speaker="Isabella" comment="Isabella_Base.slice">Nie zasługujesz na zaszczyt śmierci z mojej ręki.</string>
	<string key="Isabella_CombatTaunt_6" speaker="Isabella" comment="Isabella_Base.slice">Głodzę je specjalnie na taką okazję.</string>
	<string key="Isabella_CombatTaunt_8" speaker="Isabella" comment="Isabella_Base.slice">Piekło nie zna wściekłości takiej jak moja!</string>
	<string key="Isabella_CombatTaunt_19" speaker="Isabella" comment="Isabella_Base.slice">Pocieszne, jak karaluchy szturmujące bramy.</string>
	<string key="Isabella_CombatTaunt_24" speaker="Isabella" comment="Isabella_Base.slice">To powinno być zabawne.</string>
	<string key="Isabella_CombatTaunt_25" speaker="Isabella" comment="Isabella_Base.slice">Zatańcz dla mnie.</string>
	<string key="Isabella_CombatTaunt_32" speaker="Isabella" comment="Isabella_Base.slice">Czcij mnie i rozpaczaj!</string>
	<string key="Isabella_CombatTaunt_33" speaker="Isabella" comment="Isabella_Base.slice">Nikt mnie nie zatrzyma!</string>
	<string key="Isabella_CombatTaunt_34" speaker="Isabella" comment="Isabella_Base.slice">Zetrę cię na proch.</string>
	<string key="Isabella_CombatTaunt_35" speaker="Isabella" comment="Isabella_Base.slice">Błagaj na kolanach o śmierć!</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Taunt_1" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">Naiwni śmiertelnicy wkraczają do jej domeny… ślepi na swe prawdziwe przeznaczenie.</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Taunt_Gate_1" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">Wkrótce ujrzą koszmary, które im się nie śniły…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Taunt_Gate_2" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">Koszmary, na które żaden ludzki umysł nie jest gotowy…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Taunt_2" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">Będą brnąć przed siebie, dalej niż zło, dalej niż prawda…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Taunt_3" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">Potężne hordy pokiereszują ich ciała, pozbawiając ich sił…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Taunt_4" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">Języki ciemności wychłoszczą ich bezlitośnie, pozbawiając ich determinacji…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Taunt_5" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">Mimo to będą brnąć przed siebie, dalej niż smutek, dalej niż śmierć…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Hall_Intro" speaker="Isabella" comment="Isabella_Lair_Basilica.slice" rel_version="Expedition_ShatterMtn_00">Aż w końcu ujrzą ucieleśnienie swego lęku. Ujrzą orędowniczkę ich własnej zagłady…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Taunt_Alter_1" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">A ona odrzuci słabych i niegodnych…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Taunt_Alter_2" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">By mogli cierpieć w samotności.</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Relic_1" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">Czy to wszystko jest snem? Czy otwarcie tego pudełka sprawi, że się obudzę…?</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Relic_2" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">Teraz, gdy sen się zakończył… została tylko ciemność…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Relic_3" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">To ostateczna cena, jaką musi zapłacić orędowniczka…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Relic_Found" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">Pośród otchłani znajdą wątły promyk światła… pudełko, i to bardzo osobiste…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Hall_Outro" speaker="Isabella" comment="Isabella_Lair_Basilica.slice" rel_version="Expedition_ShatterMtn_00">Orędowniczka wykroczy poza ciało, umysł i ducha.</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Basilica_Intro" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">I tak oto orędowniczka powstaje, w potędze i chwale…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Basilica_Outro" speaker="Isabella" comment="Isabella_Base.slice" rel_version="Expedition_ShatterMtn_00">Nie będzie wytchnienia, nie będzie litości…</string>
	<string key="Isabella_Dungeon_ShatterMtn_00_Throne_Intro" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">W końcu ci naiwni śmiertelnicy poznają swe prawdziwe przeznaczenie. Zrozumieją, że ich dusze będą jej pokarmem!</string>
	<string key="Neishatun_Intro" speaker="Neishatun" comment="Naga_Corrupted.slice" rel_version="Expedition_ShatterMtn_00">Ani kroku dalej, robaki!</string>
	<string key="Neishatun_CombatTaunt_1" speaker="Neishatun" comment="Naga_Corrupted.slice" rel_version="Expedition_ShatterMtn_00">Pożrę was!</string>
	<string key="Neishatun_CombatTaunt_2" speaker="Neishatun" comment="Naga_Corrupted.slice" rel_version="Expedition_ShatterMtn_00">Mój głód jest wielki!</string>
	<string key="Neishatun_CombatTaunt_3" speaker="Neishatun" comment="Naga_Corrupted.slice" rel_version="Expedition_ShatterMtn_00">Zbudź się!</string>
	<string key="Neishatun_CombatTaunt_4" speaker="Neishatun" comment="Naga_Corrupted.slice" rel_version="Expedition_ShatterMtn_00">Powstań, chaosie wcielony!</string>
	<string key="Neishatun_CombatTaunt_5" speaker="Neishatun" comment="Naga_Corrupted.slice" rel_version="Expedition_ShatterMtn_00">Spalę was!</string>
	<string key="Neishatun_PhaseTransition_1" speaker="Neishatun" comment="Naga_Corrupted.slice" rel_version="Expedition_ShatterMtn_00">Nie uciekniecie przed płomieniem.</string>
	<string key="Neishatun_PhaseTransition_2" speaker="Neishatun" comment="Naga_Corrupted.slice" rel_version="Expedition_ShatterMtn_00">Burza pochłonie wasz świat.</string>
	<string key="Neishatun_PhaseTransition_3" speaker="Neishatun" comment="Naga_Corrupted.slice" rel_version="Expedition_ShatterMtn_00">Świat zostanie obsiany.</string>
	<string key="Neishatun_Death" speaker="Neishatun" comment="Naga_Corrupted.slice" rel_version="Expedition_ShatterMtn_00">Niczego nie zmieniliście! Niczego!</string>
	<string key="Neishatun_Victory" speaker="Neishatun" comment="Naga_Corrupted.slice" rel_version="Expedition_ShatterMtn_00">Kipiący chaos pochłonie wasz świat!</string>
	<string key="Isabella_Winged_CombatBark_01" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">Spełnijcie mój puchar!</string>
	<string key="Isabella_Winged_CombatBark_02" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">Uczujcie żądło próżni!</string>
	<string key="Isabella_Winged_CombatBark_03" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">Chaos kipi poprzez ziemię!</string>
	<string key="Isabella_Winged_CombatBark_04" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">Przywołuję Wir!</string>
	<string key="Isabella_Winged_PhaseTransition" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">Podziwiajcie moc, jaką mi obiecano!</string>
	<string key="Isabella_Winged_Victory" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">Wszystko się rozpada! Tylko ja pozostaję!</string>
	<string key="Isabella_Winged_Intro_01" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">W końcu ci naiwni śmiertelnicy poznają swe prawdziwe przeznaczenie.</string>
	<string key="Isabella_Winged_Intro_02" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">Zrozumieją, że ich dusze będą jej pokarmem!</string>
	<string key="Isabella_Winged_Death_01" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">Głupcy, moja moc nie ma granic!</string>
	<string key="Isabella_Winged_Death_02" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">Co…? Nie!</string>
	<string key="Isabella_Winged_Death_03" speaker="Isabella" comment="Isabella_Lair_Phase2.slice" rel_version="Expedition_ShatterMtn_00">Jestem zgubiona…</string>
	<string key="Nereid_CombatTaunt_01" speaker="@Dryad_Siren_VitalsName" comment="Dryad_Siren.slice" rel_version="Expedition_CutlassKeys_00">Pijawki lądowe! Tońcie!</string>
	<string key="Nereid_CombatTaunt_02" speaker="@Dryad_Siren_VitalsName" comment="Dryad_Siren.slice" rel_version="Expedition_CutlassKeys_00">Gińcie, kreatury!</string>
	<string key="Nereid_CombatTaunt_03" speaker="@Dryad_Siren_VitalsName" comment="Dryad_Siren.slice" rel_version="Expedition_CutlassKeys_00">Cyklonie, miażdż!</string>
	<string key="Nereid_CombatTaunt_04" speaker="@Dryad_Siren_VitalsName" comment="Dryad_Siren.slice" rel_version="Expedition_CutlassKeys_00">Wietrze, wyj! Piorunie, uderzaj!</string>
	<string key="Nereid_PhaseTransition_01" speaker="@Dryad_Siren_VitalsName" comment="Dryad_Siren.slice" rel_version="Expedition_CutlassKeys_00">Wirze, wciągnij ich w otchłań!</string>
	<string key="Nereid_PhaseTransition_02" speaker="@Dryad_Siren_VitalsName" comment="Dryad_Siren.slice" rel_version="Expedition_CutlassKeys_00">Nigdy nie dostaniecie Serca Oceanu!</string>
