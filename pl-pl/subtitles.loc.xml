	<string key="Helvan_GreekFire_line1" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">Płoń, ty tchórzliwy obłąkańcu!</string>
	<string key="Helvan_CannonFire_line1" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">Goń prawo na burt, zapchlony kundlu!</string>
	<string key="Helvan_CannonFire_line2" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">Zwiewaj do portu, tłusty szczurze!</string>
	<string key="Helvan_Death_line1" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">Nie ciesz się tak. Wkrótce będziesz jak ja!</string>
	<string key="Helvan_JumpsDown_line1" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">Ahoj, serdeńko!</string>
	<string key="Imhotep_CombatBark_01" speaker="@Imhotep_VitalsName" comment="AI_Imhotep.slice" rel_version="Territory_Brimstone_Sands">Poczuj potęgę pradawnych! Poczuj ich gniew!</string>
	<string key="Imhotep_CombatBark_02" speaker="@Imhotep_VitalsName" comment="AI_Imhotep.slice" rel_version="Territory_Brimstone_Sands">Jestem Powiernikiem Tajemnic! Strażnikiem Sekretów!</string>
	<string key="Imhotep_CombatBark_03" speaker="@Imhotep_VitalsName" comment="AI_Imhotep.slice" rel_version="Territory_Brimstone_Sands">Nawet ty nie możesz się równać z pradawnymi! Przestań ze mną walczyć!</string>
	<string key="Imhotep_CombatBark_04" speaker="@Imhotep_VitalsName" comment="AI_Imhotep.slice" rel_version="Territory_Brimstone_Sands">Zakończ tę bezsensowną bitwę! Dołącz do mnie! Razem zyskamy łaskę bogów!</string>
	<string key="Imhotep_CombatBark_05" speaker="@Imhotep_VitalsName" comment="AI_Imhotep.slice" rel_version="Territory_Brimstone_Sands">Aeternum należy do pradawnych! Pozwól im je odzyskać! Dla nas, ludzi, ta wyspa jest przekleństwem!</string>
	<string key="Heru_Intro_A" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">Stać, intruzi! Nikt nie przejdzie przez Wrota Nocy!</string>
	<string key="Heru_Intro_B" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">Precz stąd, albo posmakujecie naszego gniewu!</string>
	<string key="Heru_CombatTauntA" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">Nie jesteście tu mile widziani!</string>
	<string key="Heru_CombatTauntB" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">Poczujcie światło!</string>
	<string key="Heru_CombatTauntC" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">Zapłacicie za tę profanację!</string>
	<string key="Heru_CombatTauntD" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">Płońcie, intruzi!</string>
	<string key="Heru_Activate" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">Odradzam się na nowo, moi władcy!</string>
	<string key="Heru_Deactivate" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">Ma wola słabnie! Nie zasługuję na światło mych władców…</string>
	<string key="Heru_Doom_Intro" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">Nie uciekniecie…</string>
	<string key="Heru_Doom_Explo" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">przed gniewem władców!</string>
	<string key="Heru_Judgement" speaker="Heru" comment="AnubianGuardian_Horus_Boss.slice" rel_version="Territory_Brimstone_Sands">Słowo władców jest prawem!</string>
	<string key="Anpu_Intro" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">Stać, profani! Nie wolno wam tu być! Wasz żałosny lament będzie się niósł niczym wycie wiatru!</string>
	<string key="Anpu_CombatTauntA" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">Precz, profani!</string>
	<string key="Anpu_CombatTauntC" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">Słyszę zew mych władców.</string>
	<string key="Anpu_CombatTauntD" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">Wasza arogancja… zostanie ukarana!</string>
	<string key="Anpu_CombatTauntE" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">Ogarnia mnie ciemność! Nie wykonałem swego świętego zadania…</string>
	<string key="Anpu_CombatTauntF" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">Płońcie, intruzi!</string>
	<string key="Anpu_Activate" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">Spływa na mnie święte światło!</string>
	<string key="Anpu_BurningCharge" speaker="Anpu" comment="AnubianGuardian_Brute_Boss.slice" rel_version="Territory_Brimstone_Sands">Wrota zostaną oczyszczone!</string>
	<string key="Crassus_Intro" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Nie macie szans wobec potęgi Rzymu!</string>
	<string key="Crassus_ChaosLance" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Giń, kundlu!</string>
	<string key="Crassus_ChaosSpin" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Dość tego, szumowino!</string>
	<string key="Crassus_Corrupted_Blob" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Rzucę tę krainę na kolana!</string>
	<string key="Crassus_CorruptedPhalanx" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Jesteśmy falą, której nie da się powstrzymać!</string>
	<string key="Crassus_FireRain" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Chwała Rzymu ogarnie całe Aeternum!</string>
	<string key="Crassus_Enrage" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Wysłać te przeklęte bestie w zaświaty!</string>
	<string key="Crassus_PhaseTransition2" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Poczuj palący od środka gniew!</string>
	<string key="Crassus_PhaseTransition3" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Podziwiajcie potęgę, którą mnie obdarowano!</string>
	<string key="Crassus_ChaosVolley" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Ta potęga należy do nas, nie do bogów!</string>
	<string key="Crassus_ShieldRush" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Precz, psie!</string>
	<string key="Crassus_SplitAttack" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Zapłaciliśmy za tę potęgę naszymi duszami!</string>
	<string key="Crassus_Victory" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Chwała potędze Rzymu!</string>
	<string key="Crassus_Death" speaker="Crassus" comment="Corrupted_Legion_General_Crassus_Boss.slice" rel_version="Territory_Brimstone_Sands">Wszystko to było kłamstwem… proszę, wybacz mi, Ewanderze.</string>
	<string key="Baalphazu_Intro_Bark1" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Głupcy! Cienie i strach zacisną się wokół waszych gardeł! Lord Tenebrosus doczeka się swej straszliwej uczty!</string>
	<string key="Baalphazu_ShortBark_01" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Ogarnia cię strach!</string>
	<string key="Baalphazu_ShortBark_02" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Czuję głód!</string>
	<string key="Baalphazu_ShortBark_03" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Czas rozpocząć ucztę!</string>
	<string key="Baalphazu_MedBark_01" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Karmimy się twoim przerażeniem!</string>
	<string key="Baalphazu_MedBark_02" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Cienie pochłoną twą duszę!</string>
	<string key="Baalphazu_MedBark_03" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Przed strachem nie uciekniesz!</string>
	<string key="Baalphazu_LongBark_01" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Przybywajcie, cienie rozpaczy! Siejcie wokół terror!</string>
	<string key="Baalphazu_LongBark_02" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Zbierzemy strach zasiany przez mego straszliwego pana!</string>
	<string key="Baalphazu_LongBark_03" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Groza przepełnia tę krainę! Wasz strach na zimno jest przepyszny!</string>
	<string key="Baalphazu_EnterWeak_01" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">A niech cię!</string>
	<string key="Baalphazu_EnterWeak_02" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Bezczelny durniu!</string>
	<string key="Baalphazu_EnterWeak_03" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Zepsute mięso!</string>
	<string key="Baalphazu_ExitWeak_01" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Zgnij w ciemnościach, robaku!</string>
	<string key="Baalphazu_ExitWeak_02" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Będziesz cierpieć, łachudro!</string>
	<string key="Baalphazu_ExitWeak_03" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Niech szlag trafi waszą bezczelność!</string>
	<string key="Baalphazu_Victory_Bark1" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Tą krainą zawładną niezliczone koszmary! Mego pana czeka obfita uczta!</string>
	<string key="Baalphazu_Death_Bark1" speaker="Baalphazu" comment="HalloweenBoss.slice" rel_version="Territory_Brimstone_Sands">Mój pan się nasyci! Nie ma ucieczki przed grozą! Żadne światło nie przepędzi wiecznej ciemności…</string>
	<string key="Apophis_CombatTaunt_A" speaker="Apophis" comment="Sand_Elemental_QuestBoss.slice" rel_version="Territory_Brimstone_Sands">Staniesz przed obliczem sprawiedliwości!</string>
	<string key="Apophis_CombatTaunt_B" speaker="Apophis" comment="Sand_Elemental_QuestBoss.slice" rel_version="Territory_Brimstone_Sands">Zbyt długo znosiliśmy już twoją obecność!</string>
	<string key="Apophis_CombatTaunt_C" speaker="Apophis" comment="Sand_Elemental_QuestBoss.slice" rel_version="Territory_Brimstone_Sands">Jest we mnie głód zniszczenia!</string>
