	<string key="Helvan_CombatBark_01" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">TYMCZASOWY TEKST: Ahoj!</string>
	<string key="Helvan_CombatBark_02" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">TYMCZASOWY TEKST: Ognia! Z prawej burty!</string>
	<string key="Helvan_CombatBark_03" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">TYMCZASOWY TEKST: Ognia! Z lewej burty!</string>
	<string key="Helvan_CombatBark_04" speaker="@Undead_Navigator_Named_12MB01d_VitalsName" comment="AI_Helvan_the_Navigator.slice" rel_version="Quest_MSQ2_Revamp">TYMCZASOWY TEKST: Wy psubraty! Rozprawię się z wami osobiście.</string>
