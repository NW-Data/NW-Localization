	<string key="BrotherUmberto_Intro_Dark_Alt" speaker="@UmbertoSpirit_SubName" comment="brother_umberto.slice" rel_version="Quest_MSQ2_Brightwood">Nie oddam duszy po dobroci…</string>
	<string key="BrotherUmberto_Death_Dark" speaker="@UmbertoSpirit_SubName" comment="io_caerdun_99b_msq_10_finalseal.slice" rel_version="Quest_MSQ2_Brightwood">Jestem… wolny.</string>
	<string key="Season_02_Assassin_Ambush_1" speaker="Delirvesh the Sand Drinker" comment="Enc_Season_02_EggAmbush_02.slice" rel_version="Season_2_Perm">Nie wolno ruszać tego jaja! Broniłem go przed potężniejszymi wrogami niż ty!</string>
	<string key="Season_02_Assassin_Ambush_2" speaker="Blood of the Sands Assassin" comment="Enc_Season_02_EggAmbush_02.slice" rel_version="Season_2_Perm">To nasza szansa! Bierzcie jajo!</string>
	<string key="Season_02_Assassin_Ambush_2a" speaker="Blood of the Sands Assassin" comment="Enc_Season_02_EggAmbush_02.slice" rel_version="Season_2_Perm">Daj mi to jajo!</string>
	<string key="Season_02_Assassin_Ambush_3" speaker="@Bog_Monster_Season_02_VitalsName" comment="Enc_Season_02_EggAmbush_03.slice" rel_version="Season_2_Perm">Intruzi nie są tu mile widziani! Precz stąd.</string>
	<string key="Season_02_Assassin_Ambush_3a" speaker="@Bog_Monster_Season_02_VitalsName" comment="Enc_Season_02_EggAmbush_03.slice" rel_version="Season_2_Perm">Jesteś za blisko… Do ataku!</string>
	<string key="Season_02_Assassin_Ambush_3b" speaker="@Bog_Monster_Season_02_VitalsName" comment="Enc_Season_02_EggAmbush_03.slice" rel_version="Season_2_Perm">Powstańcie i chrońcie jajo!</string>
	<string key="Herikai_CombatBark_Intro" speaker="Herikai" comment="season_02_herikai.slice" rel_version="Season_2_Temp">Malek nas uprzedzał, że możesz przyjść po jajo! Ale przybywasz tu za późno! To nasza własność.</string>
	<string key="VOX_BarkA" speaker="Yonas" comment="yonas_msq2_combat.slice" rel_version="Quest_MSQ2_Edengrove">Nie chcę tego… powstrzymaj mnie!</string>
	<string key="VOX_BarkB" speaker="Yonas" comment="yonas_msq2_combat.slice" rel_version="Quest_MSQ2_Edengrove">Klejnot serca! Zniszcz go!</string>
	<string key="VOX_BarkB_Quick" speaker="Yonas" comment="yonas_msq2_combat.slice" rel_version="Quest_MSQ2_Edengrove">Klejnot serca! Zniszcz go!</string>
	<string key="VOX_BarkDeath" speaker="Yonas" comment="yonas_msq2_combat.slice" rel_version="Quest_MSQ2_Edengrove">Zrób to, proszę… zakończ to…</string>
	<string key="VOX_Saved" speaker="Yonas" comment="yonas_msq2_combat.slice" rel_version="Quest_MSQ2_Edengrove">Jestem ocalony. Dziękuję…</string>
	<string key="Adiana_Motherwell_Intro" speaker="Adiana" comment="enc_quest_motherwell.slice" rel_version="Quest_MSQ2_Edengrove">Kto miał czelność wkroczyć do Studni Matki? Ty? Jak śmiesz!</string>
	<string key="Adiana_Motherwell_Phase1" speaker="Adiana" comment="enc_quest_motherwell.slice" rel_version="Quest_MSQ2_Edengrove">Zakończ to szaleństwo! Ale już!</string>
	<string key="Adiana_Motherwell_Phase2" speaker="Adiana" comment="enc_quest_motherwell.slice" rel_version="Quest_MSQ2_Edengrove">Nie będziemy przez was cierpieć!</string>
	<string key="Adiana_Motherwell_Death" speaker="Adiana" comment="enc_quest_motherwell.slice" rel_version="Quest_MSQ2_Edengrove">Ten, kto mi się sprzeciwia, źle kończy. Nie zapomnę ci tego, człowieku!</string>
	<string key="Isabella_YonasTaunt_1" speaker="Isabella" comment="yonascave_99e_msq_isabella_noai.slice" rel_version="Quest_MSQ2_Edengrove">No proszę! Jesteś! Och, niestety ominęła cię cała rozrywka.</string>
	<string key="Isabella_YonasTaunt_2" speaker="Isabella" comment="yonascave_99e_msq_isabella_noai.slice" rel_version="Quest_MSQ2_Edengrove">Wyrwanie klejnotu serca jest łatwiejsze, niż sądziłam!</string>
	<string key="Isabella_YonasTaunt_3" speaker="Isabella" comment="yonascave_99e_msq_isabella_noai.slice" rel_version="Quest_MSQ2_Edengrove">Wydawało ci się, że zyskujesz przewagę, ale twoje szanse od początku były zerowe.</string>
	<string key="Isabella_YonasTaunt_4" speaker="Isabella" comment="yonascave_99e_msq_isabella_noai.slice" rel_version="Quest_MSQ2_Edengrove">Zniszczyłam wszystko, co udało ci się osiągnąć.</string>
	<string key="Isabella_YonasTaunt_5" speaker="Isabella" comment="yonascave_99e_msq_isabella_noai.slice" rel_version="Quest_MSQ2_Edengrove">Oto twój mentor, twój przyjaciel… a raczej to, co z niego zostało…</string>
