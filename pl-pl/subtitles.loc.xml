	<string key="Nereid_Victory" speaker="@Dryad_Siren_VitalsName" comment="Dryad_Siren.slice" rel_version="Expedition_CutlassKeys_00">Tońcie w swych smutkach, glisty!</string>
	<string key="AdmiralBlackPowder_Intro" speaker="@Undead_Admiral_Brute_DG_Cutlass_00_VitalsName" comment="Undead_Admiral_Brute_Boss.slice" rel_version="Expedition_CutlassKeys_00">Przychodzicie po skarby z głębin? W takim razie cierpcie tak, jak ja cierpiałem!</string>
	<string key="AdmiralBlackPowder_Victory" speaker="@Undead_Admiral_Brute_DG_Cutlass_00_VitalsName" comment="Undead_Admiral_Brute_Boss.slice" rel_version="Expedition_CutlassKeys_00">Powodzenia następnym razem! O ile nie stchórzycie. Ahahaha!</string>
	<string key="AdmiralBlackPowder_Death" speaker="@Undead_Admiral_Brute_DG_Cutlass_00_VitalsName" comment="Undead_Admiral_Brute_Boss.slice" rel_version="Expedition_CutlassKeys_00">Cieszcie się zwycięstwem, póki możecie. Czeka na was ktoś jeszcze…</string>
	<string key="AdmiralBlackPowder_PhaseTransition" speaker="@Undead_Admiral_Brute_DG_Cutlass_00_VitalsName" comment="Undead_Admiral_Brute_Boss.slice" rel_version="Expedition_CutlassKeys_00">Jak ryby w beczce!</string>
	<string key="AdmiralBlackPowder_CannonStorm" speaker="@Undead_Admiral_Brute_DG_Cutlass_00_VitalsName" comment="Undead_Admiral_Brute_Boss.slice" rel_version="Expedition_CutlassKeys_00">Utoniecie w ogniu i grzmocie!</string>
	<string key="AdmiralBlackPowder_JumpTransition" speaker="@Undead_Admiral_Brute_DG_Cutlass_00_VitalsName" comment="Undead_Admiral_Brute_Boss.slice" rel_version="Expedition_CutlassKeys_00">Złapcie mnie, jeśli potraficie!</string>
	<string key="AdmiralBlackPowder_JumpTransition_02" speaker="@Undead_Admiral_Brute_DG_Cutlass_00_VitalsName" comment="Undead_Admiral_Brute_Boss.slice" rel_version="Expedition_CutlassKeys_00">Boicie się wspiąć na takielunek?</string>
	<string key="AdmiralBlackPowder_PhaseTransition2" speaker="@Undead_Admiral_Brute_DG_Cutlass_00_VitalsName" comment="Undead_Admiral_Brute_Boss.slice" rel_version="Expedition_CutlassKeys_00">Za złoto, za chwałę!</string>
